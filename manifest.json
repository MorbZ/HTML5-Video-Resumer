{
	"manifest_version": 2,
	"name": "HTML5 Video Resumer",
	"version": "1.0",
	"description": "__MSG_appDesc__",
	"default_locale": "en",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage",
		"unlimitedStorage"
	],
	"content_scripts": [
		{
			"matches": [ "<all_urls>" ],
			"js": [ "content.js" ]
		}
	],
	"background": {
		"scripts": [ "background.js" ]
	}
}
